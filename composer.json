{
    "name": "digicademy/chf_object",
    "version": "2.0.0",
    "type": "typo3-cms-extension",
    "description": "CHF Object - Create and manage object-based research data in TYPO3",
    "authors": [
        {
            "name": "Jonatan Jalle Steller",
            "email": "jonatan.steller@adwmainz.de",
            "role": "Developer",
            "homepage": "https://www.adwmainz.de"
        },
        {
            "name": "CHF Object contributors"
        }
    ],
    "license": "GPL-2.0-or-later",
    "homepage": "https://extensions.typo3.org/extension/chf_object",
    "support": {
        "issues": "https://github.com/digicademy-chf/chf_object/issues",
        "source": "https://github.com/digicademy-chf/chf_object",
        "docs": "https://digicademy-chf.github.io/chf_object"
    },
    "keywords": [
        "TYPO3",
        "Cultural heritage",
        "Object",
        "Building",
        "Data",
        "Research",
        "Academia",
        "Academic"
    ],
    "require": {
        "typo3/cms-core": "^14",
        "typo3/cms-extbase": "^14",
        "typo3/cms-fluid": "^14",
        "typo3/cms-fluid-styled-content": "^14",
        "typo3/cms-rte-ckeditor": "^14",
        "digicademy/chf_base": "^2"
    },
    "suggest": {
        "digicademy/chf_bib": "Adds source relations",
        "digicademy/chf_gloss": "Provides a glossary component",
        "digicademy/chf_map": "Contains geodata and maps",
        "digicademy/chf_media": "Enhances media handling",
        "digicademy/chf_pub": "Provides publication relations"
    },
    "autoload": {
        "psr-4": {
            "Digicademy\\CHFObject\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "chf_object"
        }
    }
}